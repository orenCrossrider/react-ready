{
    "name": "CNP",
    "version": "1.2.0",
    "description": "Crossrider CNP with React and Redux",
    "scripts": {
        "prestart": "npm run remove-dist",
        "start": "parallelshell \"npm run lint:tools\" \"npm run test-watch\" \"npm run open:src\"",
        "start-wo-test": "parallelshell \"npm run lint:tools\" \"npm run open:src\"",
        "open:src": "babel-node tools/src_server.js",
        "open:dist": "babel-node tools/dist_server.js",
        "lint-base:tools": "eslint webpack.config.js tools",
        "lint:tools": "npm run lint-base:tools --",
        "lint:tools-teamcity": "npm run lint-base:tools -- --format './node_modules/eslint-teamcity/index.js'",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "node_modules/.bin/rimraf ./dist",
        "build:html": "babel-node tools/build_html.js",
        "prebuild": "npm run clean-dist && npm run build:html",
        "build": "npm run test && babel-node tools/build.js && npm run open:dist",
        "build-static": "babel-node tools/build.js && babel-node tools/build_html.js",
        "test-mocha": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --recursive \"./test/**/*.spec.js\" -r mock-local-storage",
        "test-mocha-watch": "npm run test-mocha -- --watch",
        "test-base": "cross-env NODE_ENV=test babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- test --recursive \"./test/**/*.spec.js\" -r mock-local-storage",
        "test": "npm run test-base --",
        "test-teamcity": "npm run test -- --reporter mocha-teamcity-reporter",
        "test-watch": "npm run test -- --watch"
    },
    "author": "Crossrider",
    "license": "MIT",
    "dependencies": {
        "axios": "0.11.0",
        "classnames": "2.2.5",
        "connect-history-api-fallback": "1.2.0",
        "deep-freeze": "0.0.1",
        "deepcopy": "0.6.1",
        "material-ui": "0.15.0",
        "object-assign": "4.1.0",
        "radium": "0.17.1",
        "react": "15.0.2",
        "react-addons-shallow-compare": "15.0.2",
        "react-addons-test-utils": "15.0.2",
        "react-body-classname": "1.0.1",
        "react-dom": "15.0.2",
        "react-mixin": "3.0.5",
        "react-redux": "4.4.5",
        "react-router": "2.4.0",
        "react-select": "1.0.0-beta13",
        "react-sticky": "5.0.2",
        "react-tap-event-plugin": "1.0.0",
        "react-virtualized-select": "0.0.4",
        "redux": "3.5.2"
    },
    "devDependencies": {
        "babel-cli": "6.8.0",
        "babel-core": "6.8.0",
        "babel-eslint": "6.0.4",
        "babel-istanbul": "0.8.0",
        "babel-loader": "6.2.4",
        "babel-plugin-react-display-name": "2.0.0",
        "babel-plugin-react-transform": "2.0.2",
        "babel-preset-es2015": "6.6.0",
        "babel-preset-react": "6.5.0",
        "babel-preset-stage-1": "6.5.0",
        "browser-sync": "2.12.5",
        "chai": "3.5.0",
        "chai-as-promised": "5.3.0",
        "cheerio": "0.20.0",
        "colors": "1.1.2",
        "cross-env": "1.0.7",
        "css-loader": "0.23.1",
        "eslint": "2.9.0",
        "eslint-loader": "1.3.0",
        "eslint-plugin-react": "5.0.1",
        "eslint-plugin-spellcheck": "0.0.7",
        "eslint-teamcity": "1.1.0",
        "estraverse-fb": "1.3.1",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "0.8.5",
        "istanbul": "0.4.3",
        "jsdom": "8.5.0",
        "mocha": "2.4.5",
        "mocha-istanbul": "0.2.0",
        "mocha-jsdom": "1.1.0",
        "mocha-shared": "0.2.0",
        "mocha-teamcity-reporter": "1.0.0",
        "mock-local-storage": "1.0.2",
        "nock": "8.0.0",
        "node-sass": "3.7.0",
        "parallelshell": "2.0.0",
        "react-transform-catch-errors": "1.0.2",
        "react-transform-hmr": "1.0.4",
        "redbox-react": "1.2.4",
        "rimraf": "2.5.2",
        "sass-loader": "3.2.0",
        "sinon": "1.17.4",
        "style-loader": "0.13.1",
        "watch": "0.18.0",
        "webpack": "1.13.0",
        "webpack-dev-middleware": "1.6.1",
        "webpack-hot-middleware": "2.10.0",
        "yargs": "4.6.0"
    },
    "keywords:": [
        "react",
        "reactjs",
        "react-router",
        "hot",
        "reload",
        "hmr",
        "live",
        "edit",
        "webpack",
        "redux",
        "flux",
        "boilerplate",
        "starter"
    ]
}
